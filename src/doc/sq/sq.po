#: ../src/userGroupCheck.py:57
#: ../src/userProperties.py:310
msgid "Please enter a user name."
msgstr "Ju lutem jepni një emër përdoruesi."

#: ../src/userGroupCheck.py:58
#, python-format
msgid "The user name must not exceed %d characters."
msgstr "Emri i përdoruesit nuk duhet të jetë më i gjatë se %d shenja."

#: ../src/userGroupCheck.py:59
#, python-format
msgid "The user name '%s' contains whitespace. Please do not include whitespace in the user name."
msgstr "Emri i përdoruesit '%s' përmban hapësira bosh. Ju lutem mos përdorni hapësira boshe te emri i përdoruesit."

#: ../src/userGroupCheck.py:60
#, python-format
msgid "The user name '%s' contains a dollar sign which is not at the end. Please use dollar signs only at the end of user names to indicate Samba machine accounts."
msgstr "Emri i përdoruesit '%s' përmban një shenjë dollari e cila nuk gjendet në fund. Ju lutem përdorni shenja dollari vetëm në fund të të emrave të përdoruesve, për të treguar llogarira Samba makine."

#: ../src/userGroupCheck.py:61
#, python-format
msgid "The user name '%(name)s' contains an invalid character at position %(position)d."
msgstr "Emri i përdoruesit '%(name)s' përmban te pozicioni %(position)d një shenjë të pavlefshme."

#: ../src/userGroupCheck.py:62
msgid "Using all numbers as the user name can cause confusion about whether the user name or numerical user id is meant. Do you really want to use a numerical-only user name?"
msgstr "Përdorimi vetëm i numrave si emër përdoruesi mund të shkaktojë paqartësi rreth faktit nëse bëhet fjalë për emrin e përdoruesit apo për id-në e tij numerike. Doni vërtet të përdorni emër përdoruesi vetëm me shifra?"

#: ../src/userGroupCheck.py:67
#, python-format
msgid "The group name must not exceed %d characters."
msgstr "Emri i grupit nuk duhet të jetë më i gjatë se %d shenja."

#: ../src/userGroupCheck.py:68
#, python-format
msgid "The group name '%s' contains whitespace. Please do not include whitespace in the group name."
msgstr "Emri i grupit '%s' përmban hapësira bosh. Ju lutem mos përdorni hapësira boshe te emri i grupit."

#: ../src/userGroupCheck.py:69
#, python-format
msgid "The group name '%(name)s' contains an invalid character at position %(position)d."
msgstr "Emri i grupit '%(name)s' përmban te pozicioni %(position)d një shenjë të pavlefshme."

#: ../src/userGroupCheck.py:70
msgid "Using all numbers as the group name can cause confusion about whether the group name or numerical group id is meant. Do you really want to use a numerical-only group name?"
msgstr "Përdorimi vetëm i numrave si emër grupi mund të shkaktojë paqartësi rreth faktit nëse bëhet fjalë për emrin e grupit apo për id-në e tij numerike. Doni vërtet të përdorni emër grupi vetëm me shifra?"

#: ../src/userGroupCheck.py:135
msgid "The password contains invalid characters.  Please use only ASCII characters."
msgstr "Fjakëkalimi përmban shenja të pavlefshme.  Ju lutem përdorni vetëm shenja ASCII."

#: ../src/userGroupCheck.py:149
#, python-format
msgid "The chosen password is too weak: %s. Do you want to use it anyway?"
msgstr "Fjalëkalimi i zgjedhur është shumë i dobët: %s. Doni të përdoret, sido që të jetë?"

#. have to check for whitespace for gecos, since whitespace is ok
#: ../src/userGroupCheck.py:160
#, python-format
msgid "The name '%s' contains invalid characters.  Please use only UTF-8 characters."
msgstr "Emri '%s' përmban shenja të pavlefshme.  Ju lutem përdorni vetëm shenja UTF-8."

#. have to check for colons since /etc/passwd is a colon delimited file
#: ../src/userGroupCheck.py:168
#, python-format
msgid "The name '%s' contains a colon.  Please do not use colons in the name."
msgstr "Emri '%s' përmban shenjën dy pika.  Ju lutem mos e përdorni shenjën dy pika në emra."

#: ../src/userGroupCheck.py:177
msgid "Please enter a home directory."
msgstr "Ju lutem jepni një drejtori hyrje."

#. have to check for colons since /etc/passwd is a colon delimited file
#: ../src/userGroupCheck.py:184
#, python-format
msgid "The directory name '%s' contains a colon.  Please do not use colons in the directory name."
msgstr "Emri '%s' i drejtorisë përmban shenjën dy pika.  Ju lutem mos e përdorni shenjën dy pika në emra drejtorish."

#: ../src/userGroupCheck.py:193
#, python-format
msgid "The directory name '%s' doesn't begin with a '/'. Please specify an absolute path for the home directory."
msgstr "Emri i drejtorisë '%s' s'fillon me një '/'. Ju lutem tregoni një shteg absolut për drejtorinë shtëpi."

#: ../src/userGroupCheck.py:199
#, python-format
msgid "The directory name '%s' has path components which are too long. Please use shorter path components for the home directory."
msgstr "Emri i drejtorisë '%s' ka përbërëse shtegu të cilat janë shumë të gjata. Ju lutem përdorni për drejtorinë shtëpi përbërëse shtegu më të shkurtra."

#: ../src/userGroupCheck.py:206
#, python-format
msgid "The directory name '%s' contains empty path components. Please specify a home directory name without empty path components."
msgstr "Emri i drejtorisë '%s' përmban përbërëse shtegu bosh. Ju lutem jepni emër drejtorie shtëpi pa përbërëse shtegu bosh."

#: ../src/userGroupCheck.py:215
#, python-format
msgid "The directory name '%s' contains illegal path components. Please don't use '.' or '..' as path components for the home directory."
msgstr "Emri i drejtorisë '%s' përmban përbërëse të jashtëligjshme shtegu. Ju lutem mos përdorni '.' or '..' si përbërëse shtegu për drejtorinë shtëpi."

